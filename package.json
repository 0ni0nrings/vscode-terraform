{
    "icon": "terraform.png",
	"name": "terraform",
	"displayName": "Terraform",
	"description": "Adds syntax highlighting for Hashicorp's Terraform",
	"version": "0.0.5",
	"publisher": "mauve",
	"engines": {
		"vscode": "^0.10.6"
	},
	"categories": [
		"Languages"
	],
	"contributes": {
		"languages": [{
			"id": "terraform",
